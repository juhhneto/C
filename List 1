  scanf("%f", &a);

  printf("Digite um valor: ");
  scanf("%f", &b);

  printf("Digite um valor: ");
  scanf("%f", &c);

  if(a>b && b>c){
    maior = a;
    meio = b;
    menor = c;
  }
  if(a>c && c>b){
    maior = a;
    meio = c;
    menor = b;
  }
  if(b>a && a>c){
    maior = b;
    meio = a;
    menor = c;
  }
  if(b>c && c>a){
    maior = b;
    meio = c;
    menor = a;
  }
  if(c>b && b>a){
    maior = c;
    meio = b;
    menor = a;
  }
  if(c>a && a>b){
    maior = c;
    meio = a;
    menor = b;
  }
return 0;

exercício 14:

float valorEntrada = 0;
float soma = 0;
float valorFinal = 0;
char opcoes = 0;

printf("Digite o valor de entrada do carro: R$");
scanf("%f", &valorEntrada);

printf("Opções:\n(a) Ar condicionado: R$ 1750,00\n(b) Pintura Metálica: R$ 800,00\n(c) Vidro Elétrico: R$ 1200,00\n(d) Direção Hidráulica: R$ 2000,00\n");

while (1){
 printf("Escolha uma das opções (digite 'e' para parar): ");
 scanf(" %c",&opcoes);
  switch(opcoes){
    case 'a':
    soma = soma + 1750;
    break;
    case 'b':
    soma = soma + 800;
    break;
    case 'c':
    soma = soma + 1200;
    break;
    case 'd':
    soma = soma + 2000;
    break;
    case 'e':
    break;
   }
  if(opcoes == 'e'){
  break;
  }
}
valorFinal = soma +valorEntrada;
printf("\nValor de entrada: R$%.2f", valorEntrada);
printf("\nPreço final: R$%.2f", valorFinal);


