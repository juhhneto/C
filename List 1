  printf("Digite um número: ");
  scanf("%f", &num2);

  if(num1>num2){
    printf("\nmaior número: %.2f", num1);
  }
  if(num2>num1){
    printf("\nmaior número: %.2f", num2);
  }
  if(num1 == num2){
    printf("ambos são iguais");
  }
return 0;

exercício 8:

  float nota1 = 0;
  float nota2 =0;
  float soma = 0;
  float media= 0;

  printf("Digite a nota da AV1: ");
  scanf("%f", &nota1);

  printf("Digite sua nota da AV2: ");
  scanf("%f",&nota2);

  soma= nota1 + nota2;
  media= soma / 2;

  printf("Nota AV1: %.2f", nota1);
  printf("\nNota AV2: %.2f", nota2);
  printf("\nMédia: %.2f",media);
  if(media>=6){
    printf("\nAP - aprovado");
  }
  if(media<5){
    printf("\nRP - reprovado");
  }
  if(media>=5 && media<6){
    printf("\nAV3");
  }
return 0;

exercício 9:

   int x = 0;
   int y = 0;
   int z = 0;

  printf("digite um número: ");
  scanf("%d", &x);
  printf("digite um número: ");
  scanf("%d", &y);
  printf("digite um número: ");
  scanf("%d", &z);

  if(x>y & y>z) {
    printf("%d, %d, %d", x,y,z);
  }
  if(x>z & z>y) {
    printf("%d, %d, %d", x,z,y);
  }
  if(y>x & x>z) {
    printf("%d, %d, %d", y,x,z);
  }
  if(y>z & z>x) {
    printf("%d, %d, %d", y,z,x);
  }
  if(z>y & y>x) {
    printf("%d, %d, %d", z,y,x);
  }
  if(z>x & x>y) {
    printf("%d, %d, %d", z,x,y);
  }
return 0;

exercício 10:

  float num1 = 0;
  float num2 = 0;
  char operador;
  float resultado = 0;

  printf("Calculador\n");
  printf("Digite o primeiro número: ");
  scanf("%f", &num1);

  printf("\nDigite o operador (+, -, *, /): ");
  scanf(" %c", &operador);

  printf("\nDigite o segundo numero: ");
  scanf("%f", &num2);

  switch(operador){
    case '+':
      resultado = num1 + num2;
      printf("Resultado = %.2f", resultado);
      break;
    case '-':
      resultado= num1 - num2;
      printf("Resultado = %.2f", resultado);
      break;
    case '*':
      resultado = num1 * num2;
      printf("Resultado = %.2f", resultado);
      break;
    case '/':
    if(num1 != 0 || num2 != 0){
      resultado = num1/num2;
      printf("Resultado = %.2f", resultado);
    }
    break;
    default:
    printf("Inválido (digite +, -, * ou / para funcionar)");
  }
return 0;

exercício 11:

  char consumidor;
  int metro = 00;
  float valorFinal = 0;

  printf("Ensira o tipo de consumidor (r - residencial, i - industrial, c -
  comercial): "); scanf(" %c", &consumidor);

  printf("Ensira quantos m³: ");
  scanf("%d", &metro);

  switch (consumidor) {
    case 'r':
      metro = metro *0.05;
      valorFinal = metro + 5;
      printf("\nConta:\nConsumidor Residencial:");
      printf("\nConsumo de água por m³ = R$" "%.2d", metro);
      printf("\nTaxa: R$5.00");
      printf("\nValor total a pagar = R$" "%.2f",valorFinal);
      break;
    case 'c':
      printf("\nConta:\nConsumidor Comercial");
      if(metro >= 80){
        metro = metro *0.25 - 80*0.25;
        valorFinal = valorFinal + 500;
        valorFinal = metro + valorFinal;
        printf("\nConsumo de água por m³ = R$" "%.2d", metro);
        printf("\nTaxa: R$500 aos primeiros 80m³");
        printf("\nValor total a pagar = R$""%.2f", valorFinal);

      } else{
        valorFinal = metro *0.25;
        printf("\nConsumo de água por m³ = R$" "%.2f", valorFinal);
        printf("\nValor total a pagar = R$""%.2f",valorFinal);
      }
      break;
    case 'i':
  printf("\nConta:\nConsumidor Industrial");
      if(metro >= 100){
        metro = metro *0.04 - 100*0.04;
        valorFinal = valorFinal + 800;
        valorFinal = metro + valorFinal;
        printf("\nConsumo de água por m³ = R$" "%.2d", metro);
        printf("\nTaxa: R$800 aos primeiros 100m³");
        printf("\nValor total a pagar = R$""%.2f", valorFinal);

      } else{
        valorFinal = metro *0.04;
        printf("\nConsumo de água por m³ = R$" "%.2f", valorFinal);
        printf("\nValor total a pagar = R$""%.2f",valorFinal);
      }
      break;
    default:
    printf("inválido");
  }
return 0;

exercício 12:

  int semana = 0;
  char categoria;
  float precoNormal = 10;
  float precoFinal = 0;

  printf("Preço normal da fita: R$" "%.2f", precoNormal);

  printf("\nEscolha o dia da semana desejado(1 - domingo, 2 - segunda, 3 -
  terça, 4 - quarta, 5 - quinta, 6 - sexta, 7 - sábado): "); scanf("%d",
  &semana);

  printf("Escolha a categoria de filme(c - comum, l - lançamento): ");
  scanf(" %c", &categoria);

  switch(semana){
    case 1:
    precoFinal = precoNormal;
    printf("Preço: R$""%.2f", precoFinal);
    break;
    case 2:
    precoFinal = precoNormal - precoNormal*0.40;
    printf("Preço: R$""%.2f", precoFinal);
    break;
    case 3:
    precoFinal = precoNormal - precoNormal*0.40;
    printf("Preço: R$""%.2f", precoFinal);
    break;
    case 4:
    precoFinal = precoNormal;
    printf("Preço: R$""%.2f", precoFinal);
    break;
    case 5:
    precoFinal = precoNormal - precoNormal*0.40;
    printf("Preço: R$""%.2f", precoFinal);
    break;
    case 6:
    precoFinal = precoNormal;
    printf("Preço: R$""%.2f", precoFinal);
    break;
    case 7:
    precoFinal = precoNormal;
    printf("Preço: R$""%.2f", precoFinal);
    break;
    default:
    printf("inválido");
  }
  precoNormal = 10;
  if(categoria == 'c'|| categoria == 'C') {
    printf("\nValor final: R$" "%.2f",precoFinal);
  }else{
    if(categoria == 'l'|| categoria == 'L'){
      precoFinal = precoFinal + precoNormal*0.15;
      printf("\nValor final: R$" "%.2f", precoFinal);
    }
   }
return 0;

