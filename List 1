  printf("digite um número (digite 0 para parar): ");
  scanf("%f", &num);
  soma = soma + num;
}
printf("soma: %.2f", soma);
return 0;


exercício 16:

  float num = 0;
  int qtdNum = 0;
  float soma = 0;
  float media = 0;

  printf("Digite números reais (digite 0 para parar): ");

  while (1){
    printf("\nNúmero: ");
    scanf("%f", &num);
    if(num ==0){
      break;
    }
    if(num>0){
      qtdNum++;
      soma = soma+ num;
    }
  }
  if(qtdNum>0){
  media= soma/qtdNum;
  printf("\nQuantidade de números: %d\n",qtdNum);
  printf("Média dos números: %.2f", media);
  }
  return 0;


exercício 17:

  int idade = 0;
  int soma = 0;
  int somaIdades = 0;
  float media = 0;
  int pessoas =0;

    printf("digite a idade das pessoas (digite 0 para encerrar):\n");
  for(int i; i!=0; i++){
    printf("Idade da pessoa: ");
    scanf("%d", &idade);
    if(idade==0){
      break;
    }
    if(idade>=18){
      pessoas ++;
      somaIdades = somaIdades + idade;

    }
  }
  if(pessoas>0){
    media = somaIdades/pessoas;
    printf("Soma das pessoas maiores de idade: %d\n", pessoas);
    printf("Média de idade das pessoas maiores de idade: %.2f\n", media);
  } else {
    printf("Nenhuma pessoa maior de idade foi informada.");
  }
  return 0;


exercício 18:

float preco = 0;
int num = 0;
float qtd = 0;
float soma = 0;
float media =0;
float valor =0;
float total =0;
int qtdnum = 0;
float maior=0;
float menor=0;

printf("Pesquisa sobre consumo mensal de energia elétrica:\n");

while (1) {

printf("\nDigite o número do consumidor: ");
scanf("%d", &num);

if(num==0){
  break;
}
qtdnum++;
printf("Digite o preço do kWh consumido: R$");
scanf("%f", &preco);

printf("Digite a quantidade de kWh consumidos durante o mês: ");
scanf("%f", &qtd);
  
if(qtd>maior){
  maior = qtd;
} 
if(menor == 0|| qtd< menor){
  menor = qtd;
}
soma= preco*qtd;
valor = soma;
total= total+soma;
printf("Valor: %.2f", valor);
}
media= total/qtdnum;

printf("\nMaior valor: %.2f", maior);
printf("\nMenor valor: %.2f", menor);
printf("\nMédia de todos: R$%.2f", media);
return 0;

exercício 19:

float num = 0;
float calculo=0;

printf("digite um número: ");
scanf("%f", &num);
printf("tabuada do %.2f\n",num);
  
for(float tab = 1; tab<=10; tab++){
  calculo = num*tab ;
  printf("%.2f x %.2f = %.2f\n", tab, num, calculo);
}
return 0;

exercício 20:

int opcao = 0;

while(1){
  printf("Bem-vindo ao menu!");
  printf("Escolha entre:\nOpção 0\nOpção 1\nDigite 2 para sair do menu");
  printf("\nOpção (0, 1, 2): ");
  scanf("%d", &opcao);

  switch(opcao){
    case 0:
    printf("opção 0 escolhida");
    break;
   case 1:
    printf("opção 1 escolhida");
    break;
    default:
    printf("fim da execução\n");
  }
  if(opcao != 2){
    return 0;
  }
 }
return 0;

